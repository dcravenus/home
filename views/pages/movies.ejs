<!DOCTYPE html>
<html>
  <head>


  <link rel="stylesheet" href="/css/movieApp.css">
  </head>

  <body>
    <div id="react-container"></div>

    <script>
      async function searchForMovie(title) {
        const response = await fetch(`/omdb?s=${title}`);
        return await response.json();
      }
    </script>
    <script src="/react/umd/react.production.min.js"></script>
    <script src="/react-dom/umd/react-dom.production.min.js"></script>

    <script src="/components/movieApp.js"></script>
  </body>
</html>
